	   <xs:element name="exeptionMessage" minOccurs="1" />
	   <xs:element name="exeptionStackTrace" minOccurs="1" />
	    </xs:sequence>
	  </xs:complexType>
	  </xs:element>
	 </xs:sequence>
	 </xs:complexType>
	 </xs:element>
</xs:schema>